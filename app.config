<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="DrDax.RadioClient.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
		</sectionGroup>
	</configSections>

	<userSettings>
		<DrDax.RadioClient.Settings>
			<!-- Pēdējā atskaņotā kanāla identifikators, piemēram, retro.3 -->
			<setting name="ChannelId" serializeAs="String">
				<value/>
			</setting>
			<!-- Skaļums no 0 (pilnīgs klusums) līdz 1 (maksimums). -->
			<setting name="Volume" serializeAs="String">
				<value>0.5</value>
			</setting>
			<!-- Vai atslēgt skaņu, kad lietotājs bloķē darbstaciju. -->
			<setting name="MuteOnLock" serializeAs="String">
				<value>True</value>
			</setting>
			<!-- Kanālu identifikatori, kurus nerāda pārslēgšanas sarakstā. Atdalīti ar tukšumu. -->
			<setting name="IgnoredChannels" serializeAs="String">
				<value/>
			</setting>
			<!-- Vai HTTP kanālam un raidījumu sarakstu izgūšanai jālieto sistēmā noteiktais starpniekserveris (tas pats, kuru izmanto Internet Explorer). -->
			<setting name="UseSystemProxy" serializeAs="String">
				<value>False</value>
			</setting>
			<!-- Vai izgūt un attēlot raidījumu sarakstu. -->
			<setting name="UseGuide" serializeAs="String">
				<value>False</value>
			</setting>
			<!-- Vai rādīt laiku studijā. -->
			<setting name="ShowClock" serializeAs="String">
				<value>True</value>
			</setting>
		</DrDax.RadioClient.Settings>
	</userSettings>

	<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/></startup>
</configuration>
